{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "required": ["nodes","outputs"],
  "properties": {
    "nodes": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "required": ["id","type","inputs"],
        "properties": {
          "id": { "type": "string", "minLength": 1 },
          "type": { "type": "string", "enum": ["EMA","SMA","RSI","MACD","ATR","ADX"] },
          "inputs": { "type": "object" },
          "params": { "type": "object" },
          "tf": { "type": "string" }
        },
        "additionalProperties": true
      }
    },
    "outputs": { "type": "object" }
  },
  "additionalProperties": false
}
