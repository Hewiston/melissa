{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "required": ["strategy_id","name","version","engine_min","created_at","permissions","assets"],
  "properties": {
    "strategy_id": { "type": "string", "minLength": 1 },
    "name": { "type": "string", "minLength": 1 },
    "version": { "type": "string", "pattern": "^[0-9]+\\.[0-9]+\\.[0-9]+$" },
    "engine_min": { "type": "string", "pattern": "^[0-9]+\\.[0-9]+\\.[0-9]+$" },
    "created_at": { "type": "string" },
    "author": { "type": "object" },
    "permissions": {
      "type": "object",
      "required": ["place_orders"],
      "properties": { "place_orders": { "type": "boolean" } },
      "additionalProperties": true
    },
    "assets": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "required": ["symbol","tf"],
        "properties": {
          "symbol": { "type": "string", "minLength": 1 },
          "tf": {
            "type": "array",
            "minItems": 1,
            "items": { "type": "string", "enum": ["1m","5m","15m","1h","4h"] }
          }
        },
        "additionalProperties": true
      }
    },
    "license": { "type": "string" },
    "hash": { "type": "string" },
    "signature_alg": { "type": "string" }
  },
  "additionalProperties": true
}
